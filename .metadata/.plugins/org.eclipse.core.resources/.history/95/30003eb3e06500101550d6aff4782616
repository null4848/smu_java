package ja0721;

public class C0721_02 {

	// 함수,메소드 - main 함수 : 모든 프로그램은 main() 함수를 찾아서 실행해주세요.
	public static void main(String[] args) {
		// 프로그램을 구현한다.
		// boolean, char, byre.short.int.long.float.double.string
		int a = 10;
		int b = 20;
//		int double = 30; // 예약어는 사용 불가
		int A = 30; // 대소문자 구분함. a,A는 다른 변수임.
//		int 1a = 10; // 숫자로 시작 불가
		int a` = 40; // 숫자가 뒤에 오는 것은 가능
		
		int a_1 = 50; // 특수문자는 _, $ 가능
		int $_a = 60; // 가능
		int a_$ = 70; // 가능
//		int a_* = 80; // 다른 특수문자 불가능

	}

}
