package ja0721;

public class C0721_04 {
	public static void main(String[] args) {
		boolean power = true;
		char ch ='A';
		char ch2 ='\u0061'; // \u:16진수를 의미 , 15*4+1*1 = 65
		char ch3 = 98;
		System.out.println("power : "+power);
		System.out.println("ch : "+ch);
		System.out.println("ch2 : "+ch2);
		System.out.println("ch3 : "+ch3);
	}

}
